      "<rootDir>/src/**/__tests__/**/*.spec.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(nanoid)/)"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.2.0",
